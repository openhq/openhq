<div class="page-header search">
  <h1>
    <span><%= pluralize(@results.count, "result") %></span> for "<%= @query %>"
  </h1>
</div>

<% if @results.any? %>
  <ul class="ui-search-list">
    <% @results.each do |result| %>
      <%= render result.class.to_s.downcase, { model: result } %>
    <% end %>
  </ul>
<% end %>